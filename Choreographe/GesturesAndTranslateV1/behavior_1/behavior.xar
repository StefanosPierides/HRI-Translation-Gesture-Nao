              <script language="4">
                <content>
                  <![CDATA[class MyClass(GeneratedClass):

    def onLoad(self):
        self.translations = {# predefined translations for spanish to english and vice versa
            "hi": "Hola",
            "hello": "Hola",
            "good morning": "Buenos das",
            "good night": "Buenas noches",
            "how are you": "Cmo ests",
            "thank you": "Gracias",
            "yes": "S",
            "no": "No",
            #---------------------------------------------------------------------------------------------------------------------------------------------------------------------
            "hola": "Hello",
            "buenos das": "Good morning",
            "buenas noches": "Good night",
            "cmo ests": "How are you",
            "gracias": "Thank you",
            "s": "Yes",
            "no": "No"

        }
        self.fallback_message = "I'm sorry, I can't translate that. Lo siento, no puedo traducir eso."
        self.log("Translation list loaded")

    def onUnload(self):
        pass

    def log(self, msg):
        print("[TranslateList] {}".format(msg))

    def Say_Output(self, text):# Handle exeption from main
        try:
            if not text or not isinstance(text, basestring):#if the text is not as expected
                text = self.fallback_message                #then set the output to the fallback message
            self.onStopped(text)                            #set output to the text
        except:
            self.onStopped(self.fallback_message)           #if unable to handle then fallback message

    def onInput_onStart(self, original_text):
        try:
            if not original_text or not isinstance(original_text, basestring):#If input is not valid
                return self.safe_output(self.fallback_message)                #then output the fallback message
            original_text = original_text.strip().lower()                     #format input
            if not original_text:                                             #check input
                return self.safe_output(self.fallback_message)                #if incorrect then output the fallback message

            self.log("Input received: '{}'".format(original_text))

            translated = self.translations.get(original_text)                 #Get the translation
            if not translated:                                                #Check translated input
                self.log("Translation not found, using fallback")
                translated = self.fallback_message                            #if incorrect then output the fallback message

            self.log("Translation: '{}'".format(translated))
            self.Say_Output(translated)                                      #Output the translated result

        except Exception as e:                                                #incase of exception
            import traceback
            tb = traceback.format_exc()
            self.Say_Output("UNHANDLED ERROR:\n{}".format(tb))]]>
